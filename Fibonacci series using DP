#include<stdio.h>

int fib(int n)
{

    int i;
    int f[n+2];
    f[0]=0;
    f[1]=1;
    for(i=2;i<=n;i++)
    {
        f[i]=f[i-1]+f[i-2];
    }
    for(i=1;i<=n;i++)
        printf("%d ",f[i]);
}

main()
{
    int n;
    printf("Enter the number to find fib series: ");
    scanf("%d",&n);
    fib(n);
}


