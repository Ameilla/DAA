#include<stdio.h>
int max,min;
int a[100];

void maxmin(int i,int j)
{
    int max1,min1,mid;
    if(i==j)
        max=min=a[i];
    else
    {
        if(i==j-1)
        {
            if(a[i]>a[j])
            {
                max=a[i];
                min=a[j];
            }
            else
            {
                max=a[j];
                min=a[i];
            }
        }
        else
        {
            mid=(i+j)/2;
            maxmin(i,mid);
            max1=max;
            min1=min;
            maxmin(mid+1,j);
            if(max<max1)
                max=max1;
            if(min>min1)
                min=min1;
        }
    }
}

main()
{
    int n,i;
    printf("Enter the number of elements: ");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
        scanf("%d",&a[i]);
    max=a[0];
    min=a[0];
    maxmin(1,n);
    printf("%d",min);
    printf(" %d",max);
}
