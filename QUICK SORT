#include<stdio.h>
void quicksort(int num[10],int first,int last)
{

    int i,j,temp,pivot;
    if(first<last)
    {
        pivot=first;
        i=first;
        j=last;
        while(i<j)
        {
            while(num[i]<=(num[pivot]&&i<last))
                i++;
            while(num[j]>num[pivot])
                j--;
            if(i<j)
            {
                temp=num[i];
                num[i]=num[j];
                num[j]=temp;
            }
        }
        temp=num[pivot];
        num[pivot]=num[j];
        num[j]=temp;
        quicksort(num,first,j-1);
        quicksort(num,j+1,last);
    }
}
main()
{
    int i,n,num[10];
    printf("Enter the number of numbers your want:");
    scanf("%d",&n);
    printf("Enter the %d numbers\n",n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&num[i]);
    }
    quicksort(num,0,n-1);
    printf("After sort \n");
    for(i=0;i<n;i++)
    {
        printf("%d ",num[i]);
    }
}
